rule UCMmap2UCMmap{
	from
		sUCMmap: Turn!UCMmap
	to
		tUCMmap: Urn!UCMmap (
			contRefs <- sUCMmap.components -> collect(c|thisModule.resolveTemp(c,'tCompRef')),
			nodes <- sUCMmap.paths -> flatten() -> collect(p|p.startPoint) -> flatten() -> collect(p|thisModule.resolveTemp(p,'tStartPoint')),
			connections <- sUCMmap.paths -> flatten() -> collect(p|p.startPoint) -> flatten() -> collect(p|thisModule.resolveTemp(p,'tNodeConn'))
		)
	do{
		thisModule.collectReferencedEnds(tUCMmap, sUCMmap.getPathNodes());
		
		--Regular ends
		--ADD CODE HERE
		for(path in sUCMmap.paths){
			thisModule.pathEnd <- path.pathBody.pathEnd;
			thisModule.newNodes <- thisModule.processPathEnd(tUCMmap, thisModule.pathEnd, false);
			
			if(thisModule.newNodes <> ''){
				tUCMmap.nodes <- tUCMmap.nodes -> union(thisModule.newNodes);	
			}

			if(thisModule.pathEnd.oclIsTypeOf(Turn!OrFork)) {
				thisModule.connectOrForkNodes(thisModule.pathEnd, tUCMmap);
			}
			else if(thisModule.pathEnd.oclIsTypeOf(Turn!AndFork)) {
				thisModule.connectAndForkNodes(thisModule.pathEnd, tUCMmap);
			}
		
			--connect nodes in a map
			thisModule.startPointName <- path.startPoint.turnGetLongestName();
			
			if(not path.startPoint.connect.oclIsUndefined() and tUCMmap.nodes -> select(n|n.oclIsTypeOf(Urn!EmptyPoint)) -> size() <> 0){
				thisModule.addPred(tUCMmap.nodes -> select(n|n.oclIsTypeOf(Urn!EmptyPoint)) -> any(ept|ept.succ -> includes(thisModule.connectMap.get(path.startPoint.connect).pred -> first())), tUCMmap.getNode(thisModule.startPointName).firstUnconnectedSucc());
			}--if no pathnodes exist
			
			if(not thisModule.pathEnd.oclIsUndefined() and thisModule.pathEnd.oclIsTypeOf(Turn!EndpointWithConnect) and path.pathBody.pathNodes -> isEmpty()){
				thisModule.endPointName <- thisModule.pathEnd.regularEnd.turnGetLongestName();
				
				if(not thisModule.pathEnd.connect.oclIsUndefined()){
					--create succ for the connecting endpoint
					thisModule.addSucc(tUCMmap.getNode(thisModule.endPointName), thisModule.createNodeConnection());
					thisModule.connect <- thisModule.createConnect(tUCMmap);
					thisModule.addPred(thisModule.connect, tUCMmap.getNode(thisModule.endPointName).succ);
					thisModule.connectMap <- thisModule.connectMap.including(thisModule.pathEnd.connect, thisModule.connect);
					if(not thisModule.pathEnd.connect.connectsTo.oclIsTypeOf(Turn!Timer)){
						thisModule.addPred(tUCMmap.getNode(thisModule.pathEnd.connect.connectsTo.name), thisModule.connect.succ);
					}
					else{
						thisModule.createdEmptyPt <- thisModule.createEmptyPt(tUCMmap, thisModule.pathEnd.connect.connectsTo, false);
						thisModule.addPred(thisModule.createdEmptyPt, thisModule.connect.succ);
					}
				}
				else if(not path.startPoint.connect.oclIsUndefined() and tUCMmap.nodes -> select(n|n.oclIsTypeOf(Urn!EmptyPoint)) -> size() <> 0){
					thisModule.addPred(tUCMmap.getNode(thisModule.endPointName), tUCMmap.nodes -> select(n|n.oclIsTypeOf(Urn!EmptyPoint)) -> any(ept|ept.succ -> includes(thisModule.connectMap.get(path.startPoint.connect).pred -> first())).firstUnconnectedSucc());
				} 
				thisModule.addPred(tUCMmap.getNode(thisModule.endPointName), tUCMmap.getNode(thisModule.startPointName).firstUnconnectedSucc());
			}--added code for stub....
			else if (not thisModule.pathEnd.oclIsUndefined() and path.pathBody.pathNodes -> isEmpty() ){
				if(thisModule.pathEnd.oclIsTypeOf(Turn!OrFork)){
					thisModule.addPred(thisModule.orForkMap.get(thisModule.pathEnd), tUCMmap.getNode(thisModule.startPointName).succ);
				}
				else if(thisModule.pathEnd.oclIsTypeOf(Turn!AndFork)){
					thisModule.addPred(thisModule.andForkMap.get(thisModule.pathEnd), tUCMmap.getNode(thisModule.startPointName).succ);
				}
				else if(thisModule.pathEnd.oclIsTypeOf(Turn!Stub)){
					thisModule.addPred(thisModule.stubMap.get(thisModule.pathEnd), tUCMmap.getNode(thisModule.startPointName).succ);
				}
				
			}
			--if pathnodes exist
			else{
				thisModule.connectNodes(path.pathBody, path, tUCMmap, OclAny);
			}
			
			thisModule.next <- tUCMmap.getNode(path.startPoint.name).succ;
			thisModule.body <- path.pathBody;
			
			if(not thisModule.body.referencedEnd.oclIsUndefined()){
				if(thisModule.body.referencedEnd.oclIsTypeOf(Turn!RespRef)){
					thisModule.addPred(thisModule.resolveTemp(thisModule.body.referencedEnd,'tRespRef'), thisModule.next);
				}			
				else{
					thisModule.addPred(tUCMmap.getNode(thisModule.body.referencedEnd.turnGetLongestName()), thisModule.next); 
				}
			}
			else{
				thisModule.connectReferencedEndToNextNode(tUCMmap, thisModule.body, thisModule.nodeCounter, thisModule.next);
			}
		}
		--Node connections
		tUCMmap.connections <- tUCMmap.nodes -> flatten() -> collect(of|of.succ) -> flatten();
		
		--TODO: move these to the declaration of tUCMmap above once code has been verified as working
		tUCMmap.id <- thisModule.nextId().toString();
		tUCMmap.name <- sUCMmap.turnGetLongestName();
	}
}